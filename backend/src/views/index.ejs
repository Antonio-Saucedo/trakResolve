    <meta name="description" content="Login to access the Trak Resolve Info API." />
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <nav>
      <ul>
        <% if(!isAuthenticated) { %>
        <li>
          <a href="/login/">Login</a>
